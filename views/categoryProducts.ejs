<!-- categoryProducts.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= category.name %> Products</title>
</head>
<body>
    <h1><%= category.name %> Products</h1>

    <ul>
        <% category.products.forEach(product => { %>
            <li><%= product.name %></li>
        <% }); %>
    </ul>
</body>
</html>
