<%- include('layout', { title: 'Product List' }) %>

<h2>Product List</h2>
<div class="product-list">
    <% products.forEach(product => { %>
        <div class="product">
            <a href="/products/<%= product._id %>">
                <h3><%= product.name %></h3>
            </a>
            
        </div>
    <% }); %>
</div>
